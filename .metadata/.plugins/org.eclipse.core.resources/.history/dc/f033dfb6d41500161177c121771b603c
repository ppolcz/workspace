package polcz.budget.service;

import java.util.List;

import javax.persistence.EntityManager;
import javax.persistence.PersistenceContext;
import javax.transaction.Transactional;

import org.springframework.stereotype.Repository;

import polcz.budget.model.TChargeAccount;

@Repository
@Transactional
public class EntityService extends AbstractService<TChargeAccount> {

	@PersistenceContext
	private EntityManager em;

	@Override
	protected EntityManager em() {
		return em;
	}

	public EntityService() {
		super(TChargeAccount.class);
	}
	
	@Override
	public void create(TChargeAccount entity) {
	    super.create(entity);
	}
	
	@Override
	public void update(TChargeAccount entity) {
	    super.update(entity);
	}
	
	@Override
	public void remove(TChargeAccount entity) {
	    super.remove(entity);
	}
	
	@Override
	public TChargeAccount find(Object id) {
	    return super.find(id);
	}
	
	@Override
	public List<TChargeAccount> findRange(int[] range) {
	    return super.findRange(range);
	}
	
	
	@Override
	public List<TChargeAccount> findAll() {
	    return super.findAll();
	}	
}